{
    "name": "tantoniazi/laravel-html-dom-parser",
    "description": "Fork Laravel wrapper for the PHP Simple HTML DOM Parser package",
    "keywords": [
        "dom parser",
        "html dom parser",
        "laravel"
    ],
    "require": {
    	"php": ">= 7.3",
        "kub-at/php-simple-html-dom-parser": "^1.9"
    },
    "homepage": "https://github.com/tantoniazi/html-dom-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Tiago Antoniazi Del Guerra",
            "email": "tiago.antoniazi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Tantoniazi\\LaravelHtmlDomParser\\": "" }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tantoniazi\\LaravelHtmlDomParser\\ServiceProvider"
            ],
            "aliases": {
                "HTMLDomParser": "Tantoniazi\\LaravelHtmlDomParser\\Facade"
            }
        }
    }
}
